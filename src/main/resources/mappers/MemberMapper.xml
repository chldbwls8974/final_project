<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.kh.final_project.dao.MemberDAO">
	<select id="selectMember" resultType="kr.kh.final_project.vo.MemberVO">
		select * from member where me_id = #{me_id}
	</select>
	<select id="selectMemberNumByNick" resultType="kr.kh.final_project.vo.MemberVO">
		select * from member where me_nickname = #{check}
	</select>
	<insert id="insertMember">
		insert into member(me_id, me_pw, me_email) values(#{m.me_id},#{m.me_pw},#{m.me_email})
	</insert>
</mapper>